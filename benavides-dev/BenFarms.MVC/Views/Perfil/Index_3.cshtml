@section scripts {
    <section class="scripts">
        <script type="text/javascript">

            $(document).ready(function () {
                ComponentsDateTimePickers.init();

                //ConsultaTickets
                var v = getSiteRootUrl();
                var card = $("#Card").val();
                var table = $('#dineroElectronico');

                // begin first table
                table.dataTable({
                    "responsive": true,
                    "info": false,
                    "ajax": v + "/Promotions/LoadTickets?card="+card,
			        "columns": [
				        { "data": "Fecha" },
				        { "data": "Producto" },
				        { "data": "Sucursal" },
				        { "data": "Ticket" },
				        { "data": "Importe" },
				        { "data": "DineroElectronico" }
                    ],
                    
                    // Internationalisation. For more info refer to http://datatables.net/manual/i18n
                    "language": {
                        "aria": {
                            "sortAscending": ": activate to sort column ascending",
                            "sortDescending": ": activate to sort column descending"
                        },
                        "search": "",
                        "searchPlaceholder": "Encuentra tu producto...",
                        "emptyTable": "No existen datos a mostrar",
                        "info": "Mostrando _START_ a _END_ de _TOTAL_ elementos",
                        "infoEmpty": "No se encuentran elementos",
                        "infoFiltered": "(Total de elementos: _MAX_)",
                        "lengthMenu": "Mostrar _MENU_",
                        //"search": "Buscar:",
                        "zeroRecords": "No existen coincidencias",
                        "paginate": {
                            "previous": "<",
                            "next": ">",
                            "last": "Último",
                            "first": "Primero"
                        }
                    },

                    "dom": '<"top"f>rt<"bottom"lp><"clear">',

                    "bStateSave": true, // save datatable state(pagination, sort, etc) in cookie.

                    "bLengthChange": false,
                    // set the initial value
                    "pageLength": 10,
                    "pagingType": "simple_numbers",
                    "order": [
                        [0, "asc"]
                    ] // set first column as a default sort by asc
                });
                
                //ConsultaAcumulacion
                var table1 = $('#piezasGratis');

                // begin first table
                table1.dataTable({
                    "responsive": true,
                    "info": false,
                    "ajax": v + "/Promotions/LoadAcumulacion?card=" + card,
                    "columns": [
				        { "data": "Producto" },
				        { "data": "Acumuladas" },
				        { "data": "Regla" },
				        { "data": "Vigencia" }
                    ],

                    // Internationalisation. For more info refer to http://datatables.net/manual/i18n
                    "language": {
                        "aria": {
                            "sortAscending": ": activate to sort column ascending",
                            "sortDescending": ": activate to sort column descending"
                        },
                        "search": "",
                        "searchPlaceholder": "Encuentra tu producto...",
                        "emptyTable": "No existen datos a mostrar",
                        "info": "Mostrando _START_ a _END_ de _TOTAL_ elementos",
                        "infoEmpty": "No se encuentran elementos",
                        "infoFiltered": "(Total de elementos: _MAX_)",
                        "lengthMenu": "Mostrar _MENU_",
                        //"search": "Buscar:",
                        "zeroRecords": "No existen coincidencias",
                        "paginate": {
                            "previous": "<",
                            "next": ">",
                            "last": "Último",
                            "first": "Primero"
                        }
                    },

                    "dom": '<"top"f>rt<"bottom"lp><"clear">',

                    "bStateSave": true, // save datatable state(pagination, sort, etc) in cookie.

                    "bLengthChange": false,
                    // set the initial value
                    "pageLength": 10,
                    "pagingType": "simple_numbers",
                    "order": [
                        [0, "asc"]
                    ] // set first column as a default sort by asc
                });
                
                //ConsultarPromocionesDisponibles
                var table2 = $('#misPromociones');

                // begin first table
                table2.dataTable({
                    "responsive": true,
                    "info": false,
                    "ajax": v + "/Promotions/LoadPromocionesDisponibles?card=" + card,
                    "columns": [
				        { "data": "Cupon" },
				        { "data": "Promocion" },
