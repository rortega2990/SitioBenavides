<section class="perfil tab-content">
    <div id="perfil-mi-cuenta" class="tab-pane active" role="tabpanel">
        <div class="container">
            <div class="form_info_01">
                <form class="form-horizontal bg-color-blue-marine">
                    @Html.AntiForgeryToken()
                    <div class="row">
                        <div class="col-sm-4">
                            <img class="img-responsive img-circle" src="@Url.Content(@Model.UserImage)" alt="">
                        </div>
                        <div class="col-sm-8">
                            <legend class="color-blue">¡Hola, @Model.Names!</legend>
                            <dl>
                                <dt>Tu saldo es de:</dt>
                                <dd>$@Model.Mount</dd>
                                <dt>Tu número de tarjeta es:</dt>
                                <dd>@Model.Card</dd>
                            </dl>
                        </div>
                    </div>
                </form>
            </div>
            <div class="form_info_02">
                <form class="form-vertical" action="" method="POST" role="form" id="perfilForm" enctype="multipart/form-data">
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.Card)
                    <legend>Perfil</legend>

                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.Names, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.Names, new { @class = "form-control input-sm", id = "Names", name = "Names", minlength = "2", maxlength = "100", required = true  })
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(m => m.Email, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.Email, new { @class = "form-control input-sm", id = "Email", name = "Email", maxlength = "150", required = true })
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(m => m.Card, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.Card, new { @class = "form-control input-sm", id = "Card", name = "Card", required = true })
                            </div>

                            <div class="form-group">
                                <a data-toggle="modal" data-dismiss="modal" href="#modal-change-password">Cambia tu contraseña</a>
                            </div>

                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.LastName1, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.LastName1, new { @class = "form-control input-sm", id = "LastName1", name = "LastName1", minlength = "2", maxlength = "100", required = true })
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(m => m.CGenre, new { @class = "control-label", style = "border:0px" })
                                <div class="col-xs-12 text-left" style="margin-bottom: 19px">
                                    @{
                                        var sexFem = Model.CGenre == Genre.Female;
                                        var sexMas = Model.CGenre == Genre.Male;
                                    }
                                    <label class="radio-inline" for="generoM">
                                        @Html.RadioButton("CGenre", "1", sexMas, new { id = "generoM", required = true })
                                        <img src="~/Content/rsc/icons/hombre.png" alt="">
                                    </label>
                                    <label class="radio-inline" for="generoF">
                                        @Html.RadioButton("CGenre", "2", sexFem, new { id = "generoF", required = true })
                                        <img src="~/Content/rsc/icons/mujer.png" alt="">
                                    </label>
                                </div>
                                <label for="CGenre" class="error"></label>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(m => m.BirthDate, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.BirthDate, new { @class = "form-control form-control-inline input-sm date-picker", id = "BirthDate", name = "BirthDate", required = true, value = Model.BirthDate })
                            </div>

                            <div class="form-group">
                                <a data-toggle="tab" aria-controls="perfil-contacto" href="#perfil-contacto">¿Extraviaste tu tarjeta?</a>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group">
                                @Html.LabelFor(m => m.LastName2, new { @class = "control-label", style = "border:0px" })
                                @Html.TextBoxFor(m => m.LastName2, new { @class = "form-control input-sm", id = "LastName2", name = "LastName2", minlength = "2", maxlength = "100", required = true })
                            </div>

                            <div class="form-group">
                                @*<div class="col-sm-6 col-xs-12">*@
                                <label class="control-label text-left">Imagen de Perfil</label>
                                <div class="text-left">
                                    <div class="form-group">
                                        <div class="fileinput fileinput-new" data-provides="fileinput">
                                            <div class="fileinput-preview thumbnail" data-trigger="fileinput">
                                                <img src="@Url.Content(Model.UserImage)" />
                                            </div>
                                            <div>
                                                <span class="btn blue btn-outline btn-file">
                                                    <span class="fileinput-new"> Seleccionar Imagen </span>
                                                    <span class="fileinput-exists"> Cambiar </span>
                                                    <input type="file" name="ImagenUsuarioRegistro" id="ImagenUsuarioRegistro">
                                                </span>
                                                <a href="javascript:;" class="btn red-soft fileinput-exists" data-dismiss="fileinput" style="margin-right:10px"> Eliminar </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @*</div>*@
                            </div>

                            <div class="form-group">
                                <button type="submit" class="btn btn-default btnEdit">Edita tus datos</button>
                            </div>



                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
