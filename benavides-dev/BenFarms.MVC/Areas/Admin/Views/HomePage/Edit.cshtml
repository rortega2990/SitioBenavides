@model BenavidesFarm.DataModels.Models.Pages.HomePage

@{
    ViewBag.Title = "Editar Página de Inicio";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    ViewBag.Page = "HomePageEdit";
}

@helper DisplayImageFileSelectionSection(string imageFileName, string controlName, string caption)
{
    @*<div class="form-group">*@
    <label class="control-label">@caption</label>
        <br />
        <div class="fileinput fileinput-new" data-provides="fileinput" style="margin-bottom:10px">
            <div class="fileinput-preview thumbnail" data-trigger="fileinput" style="width: 200px; height: 200px">
                <img src="@Url.Content(imageFileName)" />
            </div>
            <div>
                <span class="btn red btn-outline btn-file">
                    <span class="fileinput-new"> Seleccionar Imagen </span>
                    <span class="fileinput-exists"> Cambiar </span>
                    <input type="file" name="@controlName" id="@controlName">
                </span>
                <a href="javascript:;" class="btn red fileinput-exists" data-dismiss="fileinput"> Eliminar </a>
            </div>
        </div>
        @*</div>*@
}

@helper DisplayQuadEditSection(BenavidesFarm.DataModels.Models.Pages.Sections.SingleQuadSectionItem singleQuadItem, int quadNumber)
{
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12 col-md-12 text-center" style="background-color:#e7e7e7">
                <h4 style="color:black">Cuadro número @quadNumber</h4>
            </div>

            <div class="col-xs-12 col-md-9">
                <label class="control-label">Texto</label>
                <input type="text" class="form-control" name='@String.Concat("QuadText",quadNumber)' id='@String.Concat("QuadText",quadNumber)' value="@singleQuadItem.Text" required />
            </div>
            <div class="col-xs-12 col-md-3">
                <label class="control-label">Fondo</label>
                <input class="form-control demo minicolors-input" type="text"
                       name='@String.Concat("QuadBackgroundColor",quadNumber)' id='@String.Concat("QuadBackgroundColor",quadNumber)'
                       data-control="wheel" value="@Html.Raw(singleQuadItem.BackgroundColor)" required />
            </div>

            <div class="col-xs-12 col-md-12">
                <label class="control-label">Vínculo</label>
                <input type="text" class="form-control" name='@String.Concat("QuadLink",quadNumber)' value="@singleQuadItem.Link" id='@String.Concat("QuadLink",quadNumber)' required />
            </div>

            <div class="col-xs-12 col-md-12">
                @DisplayImageFileSelectionSection(singleQuadItem.ImageFileName, String.Concat("QuadImageFileName", quadNumber), "Imagen")
            </div>
        </div>
    </div>
}

